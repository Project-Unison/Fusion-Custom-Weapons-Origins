"VertexlitGeneric"
{
	"$basetexture" "models\workshop\weapons\c_items\c_rose_gun.vtf"
	"$bumpmap"    "models\workshop\weapons\c_items\c_rose_gun_normal.vtf"


	"$basemapalphaphongmask" "0"
	//"$envmap" "env_cubemap"
	//"$envmaptint" "[.3 .3 .3]"

	"$phong" "1"
	"$phongexponent" "25"
	"$phongboost" "1.2"	
	"$lightwarptexture" "models\lightwarps\weapon_lightwarp"
	"$phongfresnelranges"	"[.25 .5 1]"

	"$rimlight" "1"
	"$rimlightexponent" "10"	
	"$rimlightboost" "1.5"

	"$ambientocclusion" "1"

	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"Proxies"
	{
		"weapon_invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
	}
}

